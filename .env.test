# Database Configuration
MYSQLDB_USER=test
MYSQLDB_ROOT_PASSWORD=test_password
MYSQLDB_DATABASE=test_issue_db
MYSQLDB_LOCAL_PORT=3308
MYSQLDB_DOCKER_PORT=3306

# Application Configuration
NODE_LOCAL_PORT=8081
NODE_DOCKER_PORT=8080
NODE_ENV=test

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6380
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET=test-secret
JWT_EXPIRES_IN=1h

# Logging Configuration
LOG_LEVEL=error

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Monitoring
ENABLE_METRICS=false

# Cache Configuration
CACHE_TTL=0

# Feature Flags
FEATURE_FLAGS={"caching":false,"metrics":false,"rateLimiting":false,"securityHeaders":false,"tracing":false,"authentication":true,"clientIdValidation":true} 